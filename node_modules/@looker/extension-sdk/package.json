{
  "name": "@looker/extension-sdk",
  "version": "0.11.2",
  "description": "Looker Extension SDK",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "docs": "typedoc --mode file --out docs",
    "lint": "tslint --project tsconfig.json --format stylish 'src/**/*.ts'",
    "lint-fix": "tslint --fix --project tsconfig.json  --format stylish 'src/**/*.ts'",
    "prepare": "npm run build --scripts-prepend-node-path",
    "prepublishOnly": "npm run test --scripts-prepend-node-path",
    "test": "jest",
    "test-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js",
    "watch": "npm-watch build"
  },
  "author": "bryn.ryans@looker.com",
  "license": "MIT",
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^25.2.1",
    "@types/readable-stream": "^2.3.5",
    "@types/semver": "^6.2.0",
    "@types/request": "^2.48.5",
    "@types/request-promise-native": "^1.0.17",
    "jest": "^25.5.4",
    "npm-watch": "^0.6.0",
    "ts-jest": "^25.5.1",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^4.1.1",
    "tsutils": "^3.9.1",
    "typedoc": "^0.15.0",
    "typescript": "3.8.2",
    "typescript-tslint-plugin": "^0.3.1"
  },
  "dependencies": {
    "@looker/chatty": "^2.2.0",
    "@looker/sdk": "^0.3.4-beta.2",
    "deepmerge": "^4.2.2",
    "semver": "^6.3.0"
  },
  "peerDependencies": {},
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "quiet": false
    }
  }
}
